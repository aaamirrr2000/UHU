{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.SignalR": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "${JWT_KEY:replace_with_environment_variable}",
    "Issuer": "${JWT_ISSUER:aamirrashid.com}",
    "Audience": "${JWT_AUDIENCE:aamirrashid.com}"
  },
  "ConnectionStrings": {
    "Default": "${CONNECTION_STRING:replace_with_environment_variable}",
    "ControlCenter": "${CONTROLCENTER_CONNECTION_STRING:replace_with_environment_variable}"
  },
  "AllowedOrigins": "${ALLOWED_ORIGINS:http://localhost:5000,https://localhost:5001}",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      },
      "Https": {
        "Url": "https://localhost:5001"
      }
    }
  }
}
